    <link rel="stylesheet" href="{{ asset('CSS/TypeWF.css') }}">
    <title>Les Wagons Fret</title>
</head>
{% for categorie in NomCategorie %}
<body>
    <h1>Les {{categorie.nom}} </h1>
    {% endfor %}
    {% for pageproduit in TypeWagonFretBache %}
            <div class="ImagesLigne1">
                <div class="WagonBacheG">
                    <a href={{ path('produits_dieselG') }}> <img src={{ asset('Images/PageProduits/WagonFret/Bache/') ~ pageproduit.tailleG }} alt="WagonBacheG" id="WagonBacheG"></a>
